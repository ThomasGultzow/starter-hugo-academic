	number = {2},
	journal = {Journal of Health Care for the Poor and Underserved},
	author = {Cupertino, A. Paula and Richter, Kimber and Cox, Lisa Sanderson and Garrett, Susan and Ramirez, Rigoberto and Mujica, Fernando and Ellerbeck, Edward F.},
